    "@types/supertest": "^2.0.4",
    "concurrently": "^3.5.1",
    "jest": "^21.2.1",
    "nodemon": "^1.14.12",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
