    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "replacestream": "^4.0.3",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/busboy": "^0.2.3",
